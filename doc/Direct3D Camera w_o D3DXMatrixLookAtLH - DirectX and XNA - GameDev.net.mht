OrientationMatrix, pOrientation);<br>	D3DXVec3TransformNormal(&Up, &D3DX=
VECTOR3(0.0f, 1.0f, 0.0f), &OrientationMatrix);<br>	D3DXVec3TransformCoo=
rd(&At, &D3DXVECTOR3(0.0f, 0.0f, 100000.0f), &OrientationMatrix);<br>	<b=
r>	At+=3D(*pLocation);<br><br>	D3DXMatrixLookAtLH(&ViewMatrix, pLocation=
, &At, &Up);<br>	m_pDevice-&gt;SetTransform(D3DTS_VIEW, &ViewMatrix);<br=
>}<br>   </pre></DIV><!--ENDSCRIPT-->   <br><br>I rotate my camera aroun=
d its local axes in this way:<br>      <br>      <!--STARTSCRIPT--><BR><=
DIV CLASS=3Dsource><pre>            <br>HRESULT RotateLocalAxis(D3DXQUAT=
ERNION *pOrientation, D3DXVECTOR3 *pAxis, <font color=3D"blue">float</fo=
nt> Angle)<br>{<br>	<font color=3D"green">#ifdef _DEBUG<br></font>		<fon=
t color=3D"blue">if</font>(!pOrientation || !pAxis) <font color=3D"blue"=
>return</font>(D3DERR_INVALIDCALL);<br>	#end<font color=3D"blue">if</fon=
t><br><br>	D3DXMATRIX Matrix;<br>	D3DXVECTOR3 TransformedAxis;<br>	D3DXQ=
UATERNION Rotation;<br><br>	D3DXMatrixRotationQuaternion(&Matrix, pOrien=
tation);			<font color=3D"gray">// Build a matrix from the quaternion.<b=
r></font><br>	D3DXVec3TransformNormal(&TransformedAxis, pAxis, &Matrix);=
		<font color=3D"gray">// Transform the axis by the matrix.<br></font><b=
r>	D3DXQuaternionRotationAxis(&Rotation, &TransformedAxis, Angle);	<font=
 color=3D"gray">// Rotate the quaternion by the transformed axis.<br></f=
ont><br>	*pOrientation*=3DRotation;<br>        	<font color=3D"blue">ret=
urn</font>(D3D_OK);<br>}<br>            </pre></DIV><!--ENDSCRIPT--><br>=
