Drupal.comment.getCookie =3D function(name) {
  var search =3D name + '=3D';
  var returnValue =3D '';

  if (document.cookie.length > 0) {
    offset =3D document.cookie.indexOf(search);
    if (offset !=3D -1) {
      offset +=3D search.length;
      var end =3D document.cookie.indexOf(';', offset);
      if (end =3D=3D -1) {
        end =3D document.cookie.length;
      }
      returnValue =3D decodeURIComponent(document.cookie.substring(offse=
t, end).replace(/\+/g, '%20'));
    }
  }

  return returnValue;
};
