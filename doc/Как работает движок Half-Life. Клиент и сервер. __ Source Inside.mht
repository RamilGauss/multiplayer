body {
	background-color: #4f4f4f;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;

	padding: 0px;
	margin: 0px;
}
#logo {
	background: url('../../images/site/logo.jpg') 0 0 no-repeat;

	height: 117px;
	padding-left: 360px;
}
.logo_bg {
	background: url('../../images/site/bg.jpg') 0 0 repeat-x;

	height: 117px;
}
#top_menu {
	background: url('../../images/site/top_menu.gif') 0 0 repeat-x;

	width: 100%;
	height: 24px;
	padding: 2px 0px 2px 0px;

	overflow: hidden;
}
.top_menui {
	background: url('../../images/site/top_menui.gif') 100% 0 repeat-y;

	float: left;
	height: 18px;
	padding: 6px 40px 0px 40px;
}
.top_menui a {
	color: #b1b1b1;
	text-decoration: none;
}
.top_menui a:hover {
	color: #b1b1b1;
	text-decoration: underline;
}
.pblock {
	background: url('../../images/site/pattern.gif') 0 0 repeat #e25000;

	padding: 5px 5px 5px 5px;
}
#logo2 {
	background-color: #FFFFFF;
	
	height: 188px;
	
	overflow: hidden;
}
.logo2l {
	background: url('../../images/site/logo2l.jpg') 0 0 no-repeat;
	
	float: left;
	height: 188px;
	width: 500px;
}
.logo2r {
	background: url('../../images/site/logo2r.jpg') 0 0 no-repeat;
	
	float: right;
	height: 188px;
	width: 267px;
}
#left_menu {
	background-color: #4f4f4f;
	color: #c1c1c1;

	position: absolute;
	float: left;
	padding: 10px 25px 30px 0px;
	width: 165px;

	overflow: hidden;
}
#left_menu h1 {
	font-size: 13px;
	font-weight: bold;
	color: #FFFFFF;
	border-left: 6px solid #c1c1c1;	
	background-color: #3b3b3b;
	
	margin: 0px 0px 5px 0px;
	padding: 2px 0px 2px 5px;
	width: 100%;
}
#left_menu ul {
	margin: 0;
	padding: 0px 0px 20px 0px;
}
#left_menu ul li {
	font-size: 12px;
	list-style: none;
}
#left_menu ul li a {
	border-left: 6px solid #3b3b3b;
	text-decoration: none;
	color: #c1c1c1;

	width: 100%;
	display: block;
	padding: 2px 0px 2px 5px;
}
#left_menu ul li a:hover{
	color: #f0f0f0;
	border-left: 6px solid #c1c1c1;	
	background-color: #3b3b3b;
}
#main_block {
	padding: 10px 10px 20px 190px;

	overflow: hidden;
}
.block {
	background-color: #3b3b3b;
	color: #c1c1c1;

	padding: 10px 25px 10px 25px;
	font-size: 12px;
	line-height: 17px;

	overflow: hidden;
}
.block a {
	color: #f0f0f0;
	text-decoration: underline;
}
.block a:hover {
	background-color: #565656;
	color: #f0f0f0;
	text-decoration: underline;
}
.block li {
	margin-bottom: 3px;
}
.blocktitle {
	background: url('../../images/site/list.gif') 9px 9px no-repeat #3b3b3b;
	color: #c1c1c1;
	border-bottom: 1px solid #4f4f4f;
	font-size: 12px;
	font-weight: bold;
	
	height: 22px;
	padding: 10px 10px 0px 30px;
	
	overflow: hidden;
}
.blocktitle a {
	color: #f0f0f0;
	text-decoration: none;
}
.blocktitle a:hover {
	color: #f0f0f0;
	text-decoration: underline;
}
.blockfooter {
	background-color: #3b3b3b;
	color: #c1c1c1;
	border-top: 1px solid #4f4f4f;
	font-size: 10px;
	
	padding: 10px 25px 10px 25px;
	margin-bottom: 20px;
	
	overflow: hidden;
}
.blockfooter a {
	color: #f0f0f0;
	text-decoration: none;
}
.blockfooter a:hover {
	background-color: #565656;
	color: #f0f0f0;
	text-decoration: none;
}
.comments {
	background: url('../../images/site/comments.gif') 0 0 no-repeat;
	
	height: 14px;
	padding-left: 16px;
	float: right;
	
	overflow: hidden;
}
#footer {
	background: url('../../images/site/footer_bg.gif') 0 0 repeat-x #444444;
	color: #c7c7c7;

	height: 26px;
	padding: 6px 20px 0px 20px;
	text-align: center;

	overflow: hidden;
}
#footer a {
	color: #ffffff;
	text-decoration: underline;
}
#footer a:hover {
	color: #ffffff;
	text-decoration: none;
}
.imgbl {
	border: 1px solid #FFFFFF;

	margin: 10px 10px 10px 10px;
}
.chead {
	border-bottom: 1px solid #4f4f4f;

	width: 100%;
	padding-bottom: 3px;
	margin-bottom: 5px;
}
.chead a {
	color: #f0f0f0;
	text-decoration: none;
}
.chead a:hover {
	background-color: #565656;
	color: #f0f0f0;
	text-decoration: none;
}
.editbox {
	color: silver;
	background-color: transparent; 
	border: 1px solid silver;
}
#top_block {
	width: 180px;
	height: 150px;
	position: absolute;
	z-index: 4;
	top: 180px;
	right: 250px;
	display: inline-block;
	filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../../images/site/sm_block.png');
}
.top_block_c {
	width: 180px;
	height: 150px;
	filter:progid:DXImageTransform.Microsoft.Alpha(opacity=0); 
	background: url('../../images/site/sm_block.png') 0 0 no-repeat;
}
#top_block_text {
	width: 160px;
	height: 130px;
	padding: 10px 10px 10px 10px;
	position: absolute;
	z-index: 5;
	top: 180px;
	right: 250px;
	color: #FFFFFF;
	font-size: 11px;
	overflow: hidden;
}
#top_block_text a {
	color: #ffffff;
	text-decoration: none;
}
#top_block_text a:hover {
	color: #ffffff;
	text-decoration: underline;
}


.adv { margin: 1em 0; font-size: 81%; color: #CCC; }
.adv a { color: #CCC; }
.adv a:hover { color: white; background: #565656; }

