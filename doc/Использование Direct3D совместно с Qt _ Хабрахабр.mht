.html_format h3 {font-size: 137.5%;}
.html_format h4 {font-size: 120%;}
.html_format h5 {font-size: 110%;}
.html_format h6 {font-size: 100%;}
.html_format b,
.html_format strong{font-weight:bold;}
.html_format i,
.html_format em{font-style: italic;}

