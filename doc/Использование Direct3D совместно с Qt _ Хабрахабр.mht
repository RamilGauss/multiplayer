form .item.required label b{font-weight: normal;}
form .item.required label b:after{
	content: '*';
	vertical-align: top;
	font-size: 11px;
	color:#666;
}


form .item .checkbox_single{overflow: hidden;margin-bottom:10px;}
form .item .checkbox_single label{float:left;display:block;margin:0;}
form .item .checkbox_single label input{}
form .item .checkbox_in_label{}
form .item .checkbox_in_label label{display: inline;}
form .item .iframe_uploader{border:0;overflow: hidden}
