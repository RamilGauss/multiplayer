form .item input[type="password"],
form .item input[type="text"]{display:block;margin-bottom:5px;width:99%;border:1px solid #dcdcdc;padding:3px;}
form .item textarea{height:200px;width:99%;}

form .item input[type="password"]:disabled,
form .item input[type="text"]:disabled,
form .item textarea:disabled{border:1px solid #ddd;background:#eee;color:#bbb;}

form .item .error{color:red;margin-bottom:5px;display:none;}
form .item .description{font-size:11px;color:#9a9a9a;}
